   <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="tree ensembles, including random forest and gradient boosting"
            icon="icons/missing.gif"
            level-id="treeensemble"
            name="Decision Tree Ensemble"
            path="/analytics/mining"
            after="dtree"
            locked="true">
      </category>
      <category
            description="random forest and tree ensemble"
            icon="icons/missing.gif"
            level-id="randomforest"
            locked="true"
            name="Random Forest"
            path="/analytics/mining/treeensemble">
      </category>
      <category
            description="classification (tree ensembles and random forest)"
            icon="icons/missing.gif"
            level-id="classification"
            locked="true"
            name="Classification"
            path="/analytics/mining/treeensemble/randomforest">
      </category>
      <category
            after="classification"
            description="regression (tree ensembles and random forest)"
            icon="icons/missing.gif"
            level-id="regression"
            locked="true"
            name="Regression"
            path="/analytics/mining/treeensemble/randomforest">
      </category>
      <category
            description="nodes concerning gradient boosting"
            icon="icons/missing.gif"
            level-id="gradientboosting"
            locked="true"
            name="Gradient Boosting"
            path="/analytics/mining/treeensemble">
      </category>
      <category
            after="classification"
            description="regression"
            icon="icons/missing.gif"
            level-id="regression"
            locked="true"
            name="Regression"
            path="/analytics/mining/treeensemble/gradientboosting">
      </category>
      <category
            description="classification"
            icon="icons/missing.gif"
            level-id="classification"
            locked="true"
            name="Classification"
            path="/analytics/mining/treeensemble/gradientboosting">
      </category>
   </extension>
