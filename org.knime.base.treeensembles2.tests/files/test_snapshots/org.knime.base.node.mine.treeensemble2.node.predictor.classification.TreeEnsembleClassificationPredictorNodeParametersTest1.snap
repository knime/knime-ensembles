{
  "data" : {
    "model" : {
      "changePredictionColumnName" : false,
      "predictionColumnName" : "Prediction (Cluster Membership)",
      "appendPredictionConfidence" : true,
      "appendClassConfidences" : false,
      "appendModelCount" : false,
      "suffixForClassProbabilities" : "",
      "useSoftVoting" : false
    }
  },
  "schema" : {
    "type" : "object",
    "properties" : {
      "model" : {
        "type" : "object",
        "properties" : {
          "appendClassConfidences" : {
            "type" : "boolean",
            "title" : "Append individual class probabilities",
            "description" : "Appends one column per class containing its prediction confidence: the number of trees voting for\nthe class divided by the total number of trees.\n",
            "default" : false
          },
          "appendModelCount" : {
            "type" : "boolean",
            "default" : false
          },
          "appendPredictionConfidence" : {
            "type" : "boolean",
            "title" : "Append overall prediction confidence",
            "description" : "Appends a column with the confidence of the predicted class—equal to the maximum of all class\nconfidence values (which can also be appended individually).\n",
            "default" : true
          },
          "changePredictionColumnName" : {
            "type" : "boolean",
            "title" : "Change prediction column name",
            "description" : "Select this option to change the name of the column containing the prediction.",
            "default" : true
          },
          "predictionColumnName" : {
            "type" : "string",
            "title" : "Prediction column name",
            "description" : "Name of the column that will contain the prediction of the tree ensemble model.",
            "default" : "Prediction ()"
          },
          "suffixForClassProbabilities" : {
            "type" : "string",
            "title" : "Suffix for probability columns",
            "description" : "Suffix that is appended to the class probability column names.",
            "default" : ""
          },
          "useSoftVoting" : {
            "type" : "boolean",
            "title" : "Use soft voting",
            "description" : "Switches from hard voting (most votes win) to soft voting, which aggregates class probabilities from\nall trees. Requires the model to store class distributions (“Save target distribution in tree nodes”\nin the learner); enabling this without stored distributions triggers a warning.\n",
            "default" : false
          }
        }
      }
    }
  },
  "ui_schema" : {
    "elements" : [ {
      "type" : "Control",
      "scope" : "#/properties/model/properties/changePredictionColumnName",
      "options" : {
        "format" : "checkbox"
      }
    }, {
      "type" : "Control",
      "scope" : "#/properties/model/properties/predictionColumnName",
      "rule" : {
        "effect" : "ENABLE",
        "condition" : {
          "scope" : "#/properties/model/properties/changePredictionColumnName",
          "schema" : {
            "const" : true
          }
        }
      }
    }, {
      "type" : "Control",
      "scope" : "#/properties/model/properties/appendPredictionConfidence",
      "options" : {
        "format" : "checkbox"
      }
    }, {
      "type" : "Control",
      "scope" : "#/properties/model/properties/appendClassConfidences",
      "options" : {
        "format" : "checkbox"
      }
    }, {
      "type" : "Control",
      "scope" : "#/properties/model/properties/suffixForClassProbabilities",
      "rule" : {
        "effect" : "ENABLE",
        "condition" : {
          "scope" : "#/properties/model/properties/appendClassConfidences",
          "schema" : {
            "const" : true
          }
        }
      }
    }, {
      "type" : "Control",
      "scope" : "#/properties/model/properties/useSoftVoting",
      "options" : {
        "format" : "checkbox"
      }
    } ]
  },
  "persist" : {
    "type" : "object",
    "properties" : {
      "model" : {
        "type" : "object",
        "properties" : {
          "changePredictionColumnName" : {
            "configKey" : "changePredictionColumnName"
          },
          "predictionColumnName" : {
            "configKey" : "predictionColumnName"
          },
          "appendPredictionConfidence" : {
            "configKey" : "appendPredictionConfidence"
          },
          "appendClassConfidences" : {
            "configKey" : "appendClassConfidences"
          },
          "appendModelCount" : {
            "configKey" : "appendModelCount"
          },
          "suffixForClassProbabilities" : {
            "configKey" : "suffixForClassProbabilities"
          },
          "useSoftVoting" : {
            "configKey" : "useSoftVoting"
          }
        }
      }
    }
  }
}